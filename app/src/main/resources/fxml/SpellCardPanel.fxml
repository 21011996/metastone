<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" spacing="8.0" type="VBox" xmlns="http://javafx.com/javafx/8">
    <children>
        <HBox fx:id="battlecryPanel" spacing="8.0">
            <children>
                <Label alignment="TOP_LEFT" minWidth="84.0" text="Spell:"/>
                <VBox alignment="CENTER_LEFT" spacing="8.0">
                    <children>
                        <HBox spacing="8.0">
                            <children>
                                <Label minWidth="84.0" text="TargetSelection"/>
                                <ComboBox fx:id="targetSelectionBox" prefWidth="150.0"/>
                            </children>
                        </HBox>
                        <HBox layoutX="10.0" layoutY="10.0" spacing="8.0">
                            <children>
                                <Label minWidth="84.0" text="Spell"/>
                                <ComboBox fx:id="spellBox" prefWidth="150.0"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets left="16.0"/>
            </padding>
        </HBox>
    </children>
</fx:root>
