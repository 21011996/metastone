<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" styleClass="panel" spacing="4.0" type="VBox"
         xmlns="http://javafx.com/javafx/8">
    <children>
        <Label styleClass="info-small" text="Minions"/>
        <ComboBox fx:id="minionComboBox" prefWidth="200.0" promptText="&lt;select a minion&gt;"/>
        <TextField fx:id="filterMinionsTextField" prefWidth="200.0" promptText="&lt;filter minions&gt;"/>
        <Button fx:id="spawnMinionButton" mnemonicParsing="false" text="Spawn"/>
        <Button fx:id="killMinionButton" mnemonicParsing="false" text="Kill..."/>
        <Button fx:id="silenceButton" layoutX="10.0" layoutY="117.0" mnemonicParsing="false" text="Silence..."/>
    </children>
</fx:root>
